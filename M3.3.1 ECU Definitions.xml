				<scaling units="ms" expression="x*.05" to_byte="x/.05" format="0.00" fineincrement="1" coarseincrement="10" />
			</table>
			<table type="Y Axis" name="Engine Speed" storagetype="uint8" endian="little" logparam="P8">
